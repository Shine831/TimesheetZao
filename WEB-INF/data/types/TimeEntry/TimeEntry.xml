<?xml version="1.0" encoding="UTF-8"?>
<type name="TimeEntry" superclass="com.jalios.jcms.Content"
	debatable="false" unitFieldEdition="true" audienced="false"
	categoryTab="true" readRightTab="true" updateRightTab="true"
	templateTab="false" workflowTab="true" advancedTab="true"
	database="true" hbm="true" revision="true" categories="">
	<title ml="true">
		<label xml:lang="en">Title</label>
		<label xml:lang="fr">Titre</label>
	</title>

	<fields>
		<field name="projectRef" editor="link" required="true"
			compactDisplay="false" type="generated.Project" parent="false">
			<label xml:lang="fr">Projet</label>
		</field>

		<field name="taskRef" editor="link" required="false"
			compactDisplay="false" type="generated.ProjectTask" parent="false">
			<label xml:lang="fr">TÃ¢che de projet</label>
		</field>

		<field name="monday" editor="double" required="false"
			compactDisplay="false" type="double" decimals="2" default="0">
			<label xml:lang="fr">Lundi</label>
		</field>

		<field name="tuesday" editor="double" required="false"
			compactDisplay="false" type="double" decimals="2" default="0">
			<label xml:lang="fr">Mardi</label>
		</field>

		<field name="wednesday" editor="double" required="false"
			compactDisplay="false" type="double" decimals="2" default="0">
			<label xml:lang="fr">Mercredi</label>
		</field>

		<field name="thursday" editor="double" required="false"
			compactDisplay="false" type="double" decimals="2" default="0">
			<label xml:lang="fr">Jeudi</label>
		</field>

		<field name="friday" editor="double" required="false"
			compactDisplay="false" type="double" decimals="2" default="0">
			<label xml:lang="fr">Vendredi</label>
		</field>

		<field name="owner" editor="member" required="false"
			compactDisplay="false" type="com.jalios.jcms.Member" chooser="popup">
			<label xml:lang="fr">Responsable</label>
		</field>

		<field name="comment" editor="wysiwyg" required="false"
			compactDisplay="false" type="String" searchable="true" rows="5"
			cols="80" ml="true" checkHtml="true" inline="true">
			<label xml:lang="fr">Commentaire</label>
		</field>
	</fields>

	<label xml:lang="fr">Saisie horaire</label>
</type>